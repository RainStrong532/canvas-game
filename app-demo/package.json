    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "css-loader": "^0.28.11",
        "node-sass-chokidar": "^1.3.3",
        "npm-run-all": "^4.1.3",
